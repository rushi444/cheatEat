generator prisma_client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = "***"
}

model User {
    id        String   @id @default(cuid())
    email     String   @unique
    birthDate DateTime
    Post      Post?    @relation(fields: [postId], references: [id])
    postId    String?
}

model Post {
    id     String @id @default(cuid())
    author User[]
}
